#pragma code_page(65001)

#include <winnt.h>
#include <winver.h>


LANGUAGE LANG_JAPANESE, SUBLANG_DEFAULT


VS_VERSION_INFO VERSIONINFO
 FILEVERSION @VERSIONRC_FILE_VERSION_MAJOR@,@VERSIONRC_FILE_VERSION_MINOR@,@VERSIONRC_FILE_VERSION_PATCH@,@VERSIONRC_FILE_VERSION_TWEAK@
 PRODUCTVERSION @VERSIONRC_PRODUCT_VERSION_MAJOR@,@VERSIONRC_PRODUCT_VERSION_MINOR@,@VERSIONRC_PRODUCT_VERSION_PATCH@,@VERSIONRC_PRODUCT_VERSION_TWEAK@
 FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
@VERSIONRC_FILEFLAGS_DECL@
 FILEOS VOS_NT_WINDOWS32
 FILETYPE @VERSIONRC_FILETYPE@
 FILESUBTYPE @VERSIONRC_FILESUBTYPE@
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "041104b0"
        BEGIN
            VALUE "Comments", "@VERSIONRC_COMMENTS@"
            VALUE "CompanyName", "@VERSIONRC_COMPANY_NAME@"
            VALUE "FileDescription", "@VERSIONRC_FILE_DESCRIPTION@"
            VALUE "FileVersion", "<FILEVERSION>"
            VALUE "InternalName", "@VERSIONRC_INTERNAL_NAME@"
            VALUE "LegalCopyright", "@VERSIONRC_COMPANY_COPYRIGHT@"
            VALUE "OriginalFilename", "@VERSIONRC_ORIGINAL_FILENAME@"
            VALUE "ProductName", "@VERSIONRC_PRODUCT_NAME@"
            VALUE "ProductVersion", "@VERSIONRC_PRODUCT_VERSION_MAJOR@.@VERSIONRC_PRODUCT_VERSION_MINOR@.@VERSIONRC_PRODUCT_VERSION_PATCH@.@VERSIONRC_PRODUCT_VERSION_TWEAK@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x411, 1200
    END
END
